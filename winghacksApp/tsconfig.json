{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "target": "ES2020",
    "lib": [
      "ES2020",
      "DOM"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/(login)/login.js",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
